<Project Sdk="Microsoft.NET.Sdk">
  <!-- This csproj is to facilitate restoring dependencies from Maestro. These versions are pulled from eng/version.details.xml -->
  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <TargetFramework>net6.0-windows10.0.19041.0</TargetFramework>
    <Platforms>x64;x86</Platforms>
    <PlatformTarget>AnyCPU</PlatformTarget>

    <Copyright>Copyright (c) Microsoft Corporation. All rights reserved.</Copyright>
    <AssemblyTitle>Microsoft.FrameworkUdk.PackageReference</AssemblyTitle>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.FrameworkUdk">
        <IncludeAssets>None</IncludeAssets> <!-- Default is `all`, can be omitted-->
        <ExcludeAssets>all</ExcludeAssets>
    </PackageReference>
  </ItemGroup>
</Project>
